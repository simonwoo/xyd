{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@types/classnames": "^2.2.10",
    "@types/styled-components": "^5.1.0",
    "@types/swiper": "^5.3.1",
    "@umijs/hooks": "^1.9.3",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.3",
    "classnames": "^2.2.6",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "rc-banner-anim": "^2.4.4",
    "rc-queue-anim": "^1.8.5",
    "rc-scroll-anim": "^2.7.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.1.1",
    "swiper": "^5.3.0",
    "umi": "^3.2.3",
    "yorkie": "^2.0.0"
  }
}
